<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
  "http://java.sun.com/dtd/web-app_2_3.dtd" >


<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <context-param>
    <param-name>co_name</param-name>
    <param-value>Apple</param-value>
  </context-param>
  <context-param>
    <param-name>co_tel</param-name>
    <param-value>02-121-1233</param-value>
  </context-param>
  <context-param>
    <param-name>email</param-name>
    <param-value>my@apple.com</param-value>
  </context-param>
  <servlet>
    <servlet-name>SimpleServlet</servlet-name>
    <servlet-class>demo.servlet.SimpleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SimpleServlet</servlet-name>
    <url-pattern>/SimpleServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ParamServlet</servlet-name>
    <servlet-class>demo.servlet.ParamServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ParamServlet</servlet-name>
    <url-pattern>/ParamServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ChangeAtrrServlet</servlet-name>
    <servlet-class>demo.servlet.ChangeAtrrServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ChangeAtrrServlet</servlet-name>
    <url-pattern>/ChangeAtrrServlet</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>demo.servlet.listeners.RoundListener</listener-class>
  </listener>
</web-app>
